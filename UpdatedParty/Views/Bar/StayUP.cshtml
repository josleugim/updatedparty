@model UpdatedParty.Models.StayUP
@{
    ViewBag.Title = "StayUP";
    Layout = "~/Views/Shared/_Interior.cshtml";
}
<h2>
    StayUP</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>UPUser</legend>
        @Html.HiddenFor(model => model.StayUPID)
        <div class="editor-label">
            @Html.LabelFor(model => model.Promotion)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Promotion)
            @Html.ValidationMessageFor(model => model.Promotion)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.BarEvent)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.BarEvent)
            @Html.ValidationMessageFor(model => model.BarEvent)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.EventDate)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.EventDate)
            @Html.ValidationMessageFor(model => model.EventDate)
        </div>
        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

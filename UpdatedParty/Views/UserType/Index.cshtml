@model IEnumerable<UpdatedParty.Models.UserType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            RegisterDate
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegisterDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.UpUserTypeId }) |
            @Html.ActionLink("Details", "Details", new { id = item.UpUserTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.UpUserTypeId })
        </td>
    </tr>
}

</table>
